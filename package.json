{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@antoniel/react-native-to-jest-preview",
  "author": "Antoniel Magalhaes",
  "module": "dist/react-native-to-jest-preview.esm.js",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "size-limit": [
    {
      "path": "dist/react-native-to-jest-preview.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-native-to-jest-preview.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "pretty-format": "^29.4.3"
  }
}
